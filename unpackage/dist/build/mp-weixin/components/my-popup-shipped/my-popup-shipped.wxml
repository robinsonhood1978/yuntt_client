<view class="uni-popup-dialog data-v-4c9e11ee"><view class="uni-dialog-title data-v-4c9e11ee"><text class="uni-dialog-title-text data-v-4c9e11ee">{{title}}</text></view><view class="pd10 ml10 data-v-4c9e11ee"><block wx:if="{{companys.length>0}}"><view class="uni-flex uni-row pb10 data-v-4c9e11ee"><text class="data-v-4c9e11ee">物流公司</text><picker class="ml10 width-surplus data-v-4c9e11ee" mode="selector" range-key="name" range="{{companys}}" value="{{selected}}" data-event-opts="{{[['change',[['picker',['$event']]]]]}}" bindchange="__e"><view class="data-v-4c9e11ee">{{companys[selected].name}}</view></picker><uni-icons class="mr10 data-v-4c9e11ee" vue-id="44e04862-1" type="arrowright" bind:__l="__l"></uni-icons></view></block><view class="uni-flex uni-row bt pt10 data-v-4c9e11ee"><text class="data-v-4c9e11ee">物流单号</text><input class="ml10 width-surplus mr10 pr10 data-v-4c9e11ee" maxlength="20" placeholder="填写物流单号" data-event-opts="{{[['input',[['__set_model',['','express_no','$event',[]]]]]]}}" value="{{express_no}}" bindinput="__e"/><block wx:if="{{express_no}}"><text data-event-opts="{{[['tap',[['clearIcon',['$event']]]]]}}" class="uni-icon mr10 data-v-4c9e11ee" bindtap="__e"></text></block></view></view><view class="uni-dialog-button-group data-v-4c9e11ee"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="uni-dialog-button data-v-4c9e11ee" bindtap="__e"><text class="uni-dialog-button-text data-v-4c9e11ee">{{closeText}}</text></view><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="uni-dialog-button uni-border-left data-v-4c9e11ee" bindtap="__e"><text class="uni-dialog-button-text uni-button-color data-v-4c9e11ee">{{confimText}}</text></view></view></view>